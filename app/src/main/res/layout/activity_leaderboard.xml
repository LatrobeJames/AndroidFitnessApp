        app:layout_constraintTop_toBottomOf="@+id/score1" />

    <TextView
        android:id="@+id/score4"
        android:layout_width="77dp"
        android:layout_height="22dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="11dp"
